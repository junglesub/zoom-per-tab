{
  "name": "Zoom Per Tab",
  "version": "1.02",
  "description": "Zoom one tab without affecting other tabs of the same domain!",
  "background": {
    "service_worker": "service_worker.js"
  },
  "manifest_version": 3,
  "icons": {
    "128": "icon128.png"
  },
  "permissions": ["tabs"],
  "host_permissions": ["https://*/*", "http://*/*"]
}
