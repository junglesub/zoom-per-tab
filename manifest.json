{
  "name": "Zoom Per Tab",
  "version": "1.02",
  "description": "Zoom one tab without affecting other tabs of the same domain!",
  "background": {
    "service_worker": "service_worker.js"
  },
  "manifest_version": 3,
  "icons": {
    "128": "icon128.png"
  },
  "permissions": ["tabs", "storage"],
  "host_permissions": ["https://*/*", "http://*/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon128.png"
    }
  }
}
